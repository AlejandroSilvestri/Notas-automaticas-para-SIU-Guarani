{
  "name": "NautoSiGua",
  "version": "1.4",
  "manifest_version": 3,
  "description": "Notas automáticas para SIU Guaraní.",
  "icons": {
 	  "128": "siuGuarani.png"
  },

  "content_scripts": [{
    "matches": ["https://siu.austral.edu.ar/portal/*"],
    "js": ["siu.js"]
  }],

  "web_accessible_resources": [{
    "resources": ["/selectores.json"],
    "matches": ["<all_urls>"]
  }]
}